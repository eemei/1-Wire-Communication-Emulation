#ifndef STATE_MACHINE_H
#define STATE_MACHINE_H



typedef enum{
	INIT,
	INIT_COMPLETE,
	READ,
	BIT_SEARCH
}initState;

typedef struct Event Event;
struct Event{
	EventType type;
	void *data;
};

int initSM(initState state);

#endif /* STATE_MACHINE_H */
